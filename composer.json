{
    "name": "topshelfcraft/supersort",
    "description": "...a super-duper sorting function for your Craft templates.",
    "type": "craft-plugin",
    "version": "4.0.0",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "SuperSort",
        "sort",
        "sorting",
        "array",
        "list"
    ],
    "support": {
        "docs": "https://github.com/TopShelfCraft/SuperSort"
    },
    "license": "proprietary",
    "authors": [
        {
            "name": "Top Shelf Craft (Michael Rog)",
            "homepage": "https://topshelfcraft.com"
        }
    ],
    "require": {
        "craftcms/cms": "^4.0",
        "topshelfcraft/base": "^4.0.1"
    },
    "autoload": {
        "psr-4": {
            "TopShelfCraft\\SuperSort\\": "src/"
        }
    },
    "extra": {
        "name": "SuperSort",
        "handle": "supersort",
        "class": "TopShelfCraft\\SuperSort\\SuperSort"
    }
}
